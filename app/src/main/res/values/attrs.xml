<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Button">
        <attr name="title" format="string"/>
    </declare-styleable>
    <declare-styleable name="SwiperBody">
        <attr name="position" format="string"/>
        <attr name="header" format="string"/>
    </declare-styleable>
    <declare-styleable name="DropDown">
        <attr name="hintText" format="string" default=""/>
    </declare-styleable>
    <declare-styleable name="DatePick">
        <attr name="label" format="string" />
        <attr name="dateError" format="string" />
    </declare-styleable>
    <declare-styleable name="DigitTextField">
        <attr name="digitError" format="string"/>
    </declare-styleable>
    <declare-styleable name="TextField">
        <attr name="inputText" format="string"/>
        <attr name="is_required" format="boolean"/>
        <attr name="text" format="string" localization="suggested" />
        <attr name="error" format="string" localization="suggested" />
        <attr name="showPassword" format="boolean" />
        <attr name="input_type" format="enum">
            <enum name="normal" value="0"/>
            <enum name="password" value="1"/>
            <enum name="email" value="2"/>
        </attr>
    </declare-styleable>
</resources>